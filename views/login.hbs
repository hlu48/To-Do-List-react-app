 <!-- Log in -->
 {{#if resObj.msg}}
 <div class="alert alert-danger" role="alert">
   Error:{{resObj.msg}}
 </div>
 {{/if}}
 <form method="post" enctype="application/x-www-form-urlencoded" action="login_submit">
   <div class="container mt-3 border-shrink">
     <div class="row justify-content-center">
     
      <div class="form-group">
        <label>User Name</label>
        <input type="text" name="username" value="{{resObj.username}}" class="form-control" id="emailLogIn"  placeholder="Enter username">
      </div>
     </div>
    
     <div class="row justify-content-center">
      <div class="form-group">
        <label for="passwordLogIn">Password</label>
        <input type="password" name="password" value="{{resObj.password}}" class="form-control" id="emailLogIn" placeholder="Password">
      </div>
     </div>

     <div class="form-group row justify-content-center">
      <div class="form-check">
        <input class="form-check-input" type="checkbox" id="gridCheck">
        <label class="form-check-label" for="gridCheck">
          Remember password?
        </label>
      </div>
    </div>

    <div class="row justify-content-center">
      <button type="submit" class="btn btn-primary col-md-2 mt-2">Submit</button>
     </div>

   </div>

</form>


